{
    "source": {
        "include": [
            "index.js",
            "src"
        ],
        "includePattern": ".js$"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "template": "node_modules/docdash"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}